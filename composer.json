  "require": {
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-json": "*",
    "php": "^7.2|^8.0",
    "guzzlehttp/guzzle": "~6.0",
    "ezyang/htmlpurifier": "^4.10",
    "symfony/dom-crawler": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.0",
    "symfony/var-dumper": "*"
  },
  "autoload": {
    "psr-4": {
