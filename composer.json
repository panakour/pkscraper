  "require": {
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-json": "*",
    "php": "^8.1",
    "guzzlehttp/guzzle": "~7.5.0",
    "ezyang/htmlpurifier": "^4.10",
    "symfony/dom-crawler": "^6.1",
    "panakour/phpdom": "^0.0.1"
  },
  "require-dev": {
