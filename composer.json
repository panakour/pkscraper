{
  "name": "panakour/pkscraper",
  "description": "Get whatever data you want.",
  "keywords": ["scraper"],
  "license": "MIT",
  "authors": [
    {
      "name": "Panagiots Koursaris",
      "email": "panakourweb@gmail.com"
    }
